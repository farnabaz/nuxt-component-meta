{
  "name": "nuxt-component-meta",
  "version": "0.0.3",
  "description": "Nuxt components metadata",
  "keywords": [
    "nuxt",
    "component",
    "metadata"
  ],
  "repository": "https://github.com/farnabaz/nuxt-component-meta",
  "license": "MIT",
  "exports": {
    ".": "./dist/runtime.mjs",
    "./module": "./dist/module.mjs"
  },
  "module": "./dist/runtime.mjs",
  "main": "./dist/runtime.mjs",
  "types": "./dist/runtime.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "unbuild",
    "dev": "nuxt dev example",
    "lint": "eslint --ext .js,.ts,.vue .",
    "test": "yarn lint && jest",
    "clean:example": "rm -rf example/.nuxt example/node_modules",
    "clean:app": "rm -rf src/app/node_modules src/app/.nuxt",
    "clean:dist": "rm -rf dist node_modules",
    "clean": "yarn clean:example && yarn clean:app && yarn clean:dist",
    "release": "yarn standard-version && git push --follow-tags origin main && npm publish"
  },
  "dependencies": {
    "@nuxt/kit": "npm:@nuxt/kit-edge",
    "pathe": "^0.2.0",
    "scule": "^0.2.1",
    "vue-docgen-api": "^4.41.2"
  },
  "devDependencies": {
    "@docus/app": "npm:@docus/app-edge",
    "@docus/debug-theme": "^1.1.2",
    "@nuxt/test-utils": "latest",
    "@nuxtjs/eslint-config-typescript": "latest",
    "@types/jest": "^27.0.2",
    "babel-eslint": "latest",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-nuxt": "^3.0.0",
    "eslint-plugin-prettier": "^3.4.1",
    "jest": "^27.2.5",
    "playwright": "^1.15.2",
    "prettier": "^2.4.1",
    "standard-version": "^9.3.1",
    "ts-jest": "^27.0.5",
    "unbuild": "^0.5.6"
  }
}
